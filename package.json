{
  "name": "move-terraform-resources",
  "license": "Apache-2.0",
  "version": "0.2.1",
  "main": "dist/main.cjs",
  "keywords": [
    "terraform"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shinebayar-g/move-terraform-resources.git"
  },
  "bin": {
    "move-terraform-resources": "./dist/main.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "add_shebang": "perl -i -pe 's,#!/usr/bin/env tsx,#!/usr/bin/env node,g' dist/main.cjs",
    "build": "rm -rf dist && tsc && mv dist/main.js dist/main.cjs && pnpm run add_shebang",
    "prepack": "pnpm run build"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "22.10.1",
    "typescript": "5.7.2"
  },
  "dependencies": {
    "commander": "12.1.0"
  },
  "packageManager": "pnpm@9.14.4"
}
